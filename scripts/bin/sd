#!/usr/bin/env bash

# This is the entry point for all commands. It requires setting up Juan's sub:
#
#  1. brew tap juanibiapina/homebrew-taps git@github.com:juanibiapina/homebrew-taps.git
#  2. brew install juanibiapina/homebrew-taps/sub

sub --name sd --bin "${BASH_SOURCE[0]}" --relative ".." -- "$@"
